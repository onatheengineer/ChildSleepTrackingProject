{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Put Health Issue Model",
  "type": "object",
  "properties": {
    "healthissue_id": {
      "type": "string"
    },
    "subject_id": {
      "type": "string"
    },
    "refDate": {
      "type": "string"
    },
    "PROJECTCode": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "isSurvey": {
      "type": "boolean"
    },
    "initialResponse": {
      "type": "string"
    },
    "notes": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    }
  },
  "required": [
    "healthissue_id",
    "PROJECTCode"
  ],
  "additionalProperties": true
}